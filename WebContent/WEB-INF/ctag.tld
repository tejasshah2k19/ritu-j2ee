<?xml version="1.0" ?>
<taglib>

	<tlib-version>1.0</tlib-version>
	<jsp-version>2.0</jsp-version>
	 
	 <tag>
	 	<name>now</name>
	 	<tag-class>com.ctag.TimeTag</tag-class>
		<body-content>empty</body-content>
	 </tag>
	 
	 <tag>
	 	<name>print</name>
	 	<tag-class>com.ctag.PrintTag</tag-class>
	 	<body-content>JSP</body-content>
	 	<attribute>
	 		<name>count</name>
	 		<required>true</required>
	 		<rtexpravalue>true</rtexpravalue>
	 	</attribute>
	 </tag>
</taglib>